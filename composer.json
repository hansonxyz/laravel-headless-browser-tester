{
    "name": "hansonxyz/laravel-headless-browser-tester",
    "description": "Debug Laravel routes using a headless browser - captures response, console errors, XHR requests, DOM state, and screenshots",
    "keywords": ["laravel", "playwright", "testing", "debugging", "headless", "browser", "console", "xhr"],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Hanson",
            "email": "brian@hanson.xyz"
        }
    ],
    "homepage": "https://github.com/hansonxyz/laravel-headless-browser-tester",
    "support": {
        "issues": "https://github.com/hansonxyz/laravel-headless-browser-tester/issues",
        "source": "https://github.com/hansonxyz/laravel-headless-browser-tester"
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "symfony/process": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Hansonxyz\\HeadlessBrowserTester\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hansonxyz\\HeadlessBrowserTester\\HeadlessBrowserTesterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
